   "source": [
    "def searching(arr,n):\n",
    "    minimum = float('inf')\n",
    "    for i in range(n):\n",
    "        if arr[i] < minimum:\n",
    "            minimum = arr[i]\n",
    "    \n",
    "    return minimum"
   ]
