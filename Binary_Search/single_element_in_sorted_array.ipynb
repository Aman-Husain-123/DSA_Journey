   "source": [
    "arr = [1,1,2,2,3,4,4]\n",
    "n = len(arr)\n",
    "ans = None\n",
    "\n",
    "# If the array contains only one element, return it\n",
    "if n == 1:\n",
    "    ans = arr[0]\n",
    "else:\n",
    "    for i in range(n):\n",
    "        if arr.count(arr[i]) == 1:\n",
    "            ans = arr[i]\n",
    "            break\n",
    "\n",
    "print(ans)\n"
   ]
  }
