    "def zigzag(n):\n",
    "    if n == 0:\n",
    "        return\n",
    "    \n",
    "    print(n,end =\"\")\n",
    "    zigzag(n-1)\n",
    "    print(n,end =\"\")\n",
    "    zigzag(n-1)\n",
    "    print(n,end =\"\")"
   ]
