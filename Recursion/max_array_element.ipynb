    "def printMax(arr,n,idx,max):\n",
    "    if idx == n:\n",
    "        print(max)\n",
    "        return\n",
    "    if max < arr[idx]:\n",
    "        max = arr[idx]\n",
    "    \n",
    "    printMax(arr,n,idx + 1,max)"
   ]
