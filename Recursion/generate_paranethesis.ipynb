   "source": [
    "def generate(strr,open,close,n):\n",
    "    if close == n:\n",
    "        print(strr)\n",
    "        return\n",
    "    \n",
    "    if open < n:\n",
    "        generate(strr + '(',open + 1,close,n)\n",
    "    \n",
    "    if close < open:\n",
    "        generate(strr + ')',open,close + 1,n)"
   ]
