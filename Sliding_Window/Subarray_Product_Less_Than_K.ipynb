   "source": [
    "nums = [10,5,2,6]\n",
    "k = 100\n",
    "\n",
    "max_len = float('inf')\n",
    "\n",
    "\n",
    "for i in range(len(nums)):\n",
    "    curr_prod = 1\n",
    "    for j in range(i+1,len(nums)):\n",
    "        if nums[i] * nums[j] < k:\n",
    "            prod = nums[i] * nums[j]\n",
    "            curr_prod *= prod\n",
    "        max_len += 1\n"
   ]
