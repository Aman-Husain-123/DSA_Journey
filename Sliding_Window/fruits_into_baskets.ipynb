   "source": [
    "arr = [3,3,3,1,2,1,1,2,3,3,4]\n",
    "maxLen = float('-inf')\n",
    "n = len(arr)\n",
    "\n",
    "for i in range(n):\n",
    "    stt = set()\n",
    "    for j in range(i,n):\n",
    "        stt.add(arr[j])\n",
    "        if len(stt) <= 2:\n",
    "            maxLen = max(maxLen,j-i+1)\n",
    "        else:\n",
    "            break"
