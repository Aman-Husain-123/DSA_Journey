   "source": [
    "arr = [10, 20, 30, 40, 50, 60]\n",
    "\n",
    "i = 0\n",
    "j = len(arr) - 1\n",
    "\n",
    "while i < j:  # swap until the middle\n",
    "    arr[i], arr[j] = arr[j], arr[i]\n",
    "    i += 1\n",
    "    j -= 1\n",
    "\n",
    "print(arr)  # [60, 50, 40, 30, 20, 10]\n"
   ]
  }
