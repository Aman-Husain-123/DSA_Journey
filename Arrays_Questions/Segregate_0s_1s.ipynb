   "source": [
    "arr = [0, 0, 1, 1, 0]\n",
    "left = 0\n",
    "right = len(arr) - 1\n",
    "\n",
    "while left < right:\n",
    "    if arr[left] == 0:\n",
    "        left += 1\n",
    "    else:\n",
    "        arr[left], arr[right] = arr[right], arr[left]\n",
    "        right -= 1"
   ]
